{
  "manifest_version": 2,

  "name": "coshop",
  "description": "This extension sends an email.",
  "version": "1.0",

  "page_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "https://www.googleapis.com/auth/gmail.send"
  ]
}
